{
  "ts-node": {
    "files": true
  },
  "exclude": ["disabled"],
  "compilerOptions": {
      "target": "ES2016",
      "module": "commonjs",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "typeRoots": ["./src/typings"]
  }
}